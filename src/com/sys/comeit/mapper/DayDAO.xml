<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.sys.comeit.idao.IDayDAO">

	<select id="dayList" resultType="com.sys.comeit.dto.DayDTO">
		SELECT DAY_CD, DAY_NAME
		FROM TBL_DAY
		ORDER BY DAY_CD
	</select>
	
</mapper>